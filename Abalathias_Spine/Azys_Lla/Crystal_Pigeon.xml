<Profile>
	<Name>Crystal Pigeon</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

		<If condition="not HasAtLeast(17581,1)">
			<Log Message="Start Fishing: Crystal Pigeon..." />
			<If condition="not HasAtLeast(12712,50)">
				<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':12712,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
			</If>
			<If condition="not IsOnSubMap(TEMP-subMapID)">
				<!-- Lisbeth Travel Cords -->
			</If>
			<If Condition="not ClassName == ClassJobType.Fisher">
				<ChangeClass Job="Fisher" />
			</If>
			<ExFish Bait="Giant Crane Fly" Condition="HasAtLeast(12712,1) and not HasAtLeast(17581,1) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(TEMP-zoneID, 'Thunder') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(TEMP-zoneID, TimeSpan.FromHours(-8), 'Fair Skies')"
					MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True"
                    Mooch="2" Patience="Patience2" MinimumGPPatience="650" ForceCordial="True" CordialType="Auto">
				<PatienceTugs>
					<PatienceTug moochLevel="0" TugType="Heavy" />
                    <PatienceTug moochLevel="1" TugType="Heavy" />
				</PatienceTugs>
				<Keepers>
					<Keeper name="Crystal Pigeon" />
				</Keepers>
				<FishSpots>
					<!-- FISH CORDS -->
				</FishSpots>
			</ExFish>
		</If>
		<LLoadProfile Path="../../Start.xml"/>

	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>