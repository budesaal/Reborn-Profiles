<?xml version="1.0" encoding="utf-8"?>
<!--
	Profile: Aphotic Pirarucu (15638)
	Authors: 
-->
<Profile>
	<Name>TEMP</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

		<If condition="not HasAtLeast(15638,1)">
			<Log Message="Start Fishing: Aphotic Pirarucu..." />
			<If condition="not HasAtLeast(12705,50)">
				<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':12705,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
			</If>
			<If condition="not IsOnSubMap(TEMP-subMapID)">
				<!-- Lisbeth Travel Cords -->
			</If>
			<If Condition="not ClassName == ClassJobType.Fisher">
				<ChangeClass Job="Fisher" />
			</If>
			<While Condition="IsTimeBetween(21,22)">
				<WaitTimer WaitTime="30" />
			</While>
			<ExFish Bait="Purse Web Spider" Condition="HasAtLeast(12705,1) and not HasAtLeast(15638,1) and IsTimeBetween(22,24) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(TEMP-zoneID, 'Clouds')"
					MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True"
					Mooch="1" Patience="Patience2" MinimumGPPatience="650" ForceCordial="True" CordialType="Auto">
				<PatienceTugs>
					<PatienceTug moochLevel="0" TugType="Heavy" />
				</PatienceTugs>
				<Keepers>
					<Keeper name="Aphotic Pirarucu" />
				</Keepers>
				<FishSpots>
					<!-- FISH CORDS -->
				</FishSpots>
			</ExFish>
		</If>
		<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>