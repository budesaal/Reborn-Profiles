			<Keepers>
				<Keeper name="Shadowstreak" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-26.53832, -10.17026, -52.92098" Heading="7,656102" Sit="True"/>
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
