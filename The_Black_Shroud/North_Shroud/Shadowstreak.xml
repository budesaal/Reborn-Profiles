<Profile>
	<Name>Shadowstreak</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

		<If condition="not HasAtLeast(7947,1)">
			<Log Message="Start Fishing: Shadowstreak..." />
			<If condition="not HasAtLeast(2624,3)">
				<Lisbeth Json="[{'Item': 2624,'Group': 0,'Amount':3,'Collectable': false,'QuickSynth': false,'SuborderQuickSynth': false,'Hq': false,'Food': 0,'Primary': true,'Type': 'Goldsmith','Enabled': true,'Manual': 0,'Medicine': 0}]"/>
			</If>
			<If condition="not IsOnSubMap(145)">
				<LisbethTravel ZoneId="154" XYZ="-30.23347, -9.153938, -46.34664"/>
			</If>
			<If Condition="not ClassName == ClassJobType.Fisher">
				<ChangeClass Job="Fisher" />
			</If>
			<While Condition="IsTimeBetween(3,4)" >
				<WaitTimer WaitTime="30" />
			</While>
			<ExFish Bait="Topwater Frog" Condition="HasAtLeast(2624,1) and not HasAtLeast(7947,1) and IsTimeBetween(4,10) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(154, 'Fog')"
					MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
				<Keepers>
					<Keeper name="Shadowstreak" />
				</Keepers>
				<FishSpots>
					<FishSpot XYZ="-26.52839, -10.12451, -52.77297" Heading="2.658334" Sit="True" />
					<FishSpot XYZ="-27.09962, -8.5765, -42.95875" Heading="6.019836" Sit="True" />
					<FishSpot XYZ="-28.64076, -12.07396, -63.1396" Heading="1.489659" Sit="True" />
				</FishSpots>
			</ExFish>
		</If>
		<LLoadProfile Path="../../Start.xml"/>

	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>