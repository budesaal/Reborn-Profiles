	<If condition="not HasAtLeast(7947,1)">
		<Log Message="Start Fishing: Shadowstreak..." />
		<If condition="not HasAtLeast(2624,3)">
			<Lisbeth Json="[{'Item': 2624,'Group': 0,'Amount':3,'Collectable': false,'QuickSynth': false,'SuborderQuickSynth': false,'Hq': false,'Food': 0,'Primary': true,'Type': 'Goldsmith','Enabled': true,'Manual': 0,'Medicine': 0}]"/>
		</If>
		<If condition="not IsOnSubMap(145)">
			<LisbethTravel ZoneId="154" XYZ="-30.23347, -9.153938, -46.34664"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While Condition="not IsTimeBetween(4,10)" >
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Topwater Frog" Condition="not HasAtLeast(7947,1) and IsTimeBetween(4,10) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(154, 'Fog')"
