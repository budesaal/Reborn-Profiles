<Profile>
	<Name>Floating Boulder</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

		<If condition="not HasAtLeast(7913,1)">
			<Log Message="Start Fishing: Floating Boulder..." />
			<If condition="not HasAtLeast(2595,50)">
				<Lisbeth Json="[{'Item': 2595,'Group': 0,'Amount':50,'Collectable': false,'QuickSynth': false,'SuborderQuickSynth': false,'Hq': false,'Food': 0,'Primary': true,'Type': 'Culinarian','Enabled': true,'Manual': 0,'Medicine': 0}]"/>
			</If>
			<If condition="not IsOnSubMap(79)">
				<LisbethTravel ZoneId="153" XYZ="-164.9751, 2.177883, 171.357"/>
			</If>
			<If Condition="not ClassName == ClassJobType.Fisher">
				<ChangeClass Job="Fisher" />
			</If>
			<While Condition="IsTimeBetween(23,24)" >
				<WaitTimer WaitTime="30" />
			</While>
			<ExFish Bait="Bass Ball" Condition="HasAtLeast(2595,1) and not HasAtLeast(7913,1) and IsTimeBetween(0,8) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(153, 'Clouds', 'Fog')"
					MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
				<Keepers>
					<Keeper name="Floating Boulder" />
				</Keepers>
				<FishSpots>
					<FishSpot XYZ="-165.4892, -0.2811633, 158.3398" Heading="3.567091" Sit="True" />
					<FishSpot XYZ="-141.2495, -0.2347637, 164.7361" Heading="3.100041" Sit="True" />
					<FishSpot XYZ="-173.4589, -0.2794424, 147.2547" Heading="0.2600002" Sit="True" />
				</FishSpots>
			</ExFish>
		</If>
		<LLoadProfile Path="../../Start.xml"/>

	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	  </CodeChunks>
</Profile>