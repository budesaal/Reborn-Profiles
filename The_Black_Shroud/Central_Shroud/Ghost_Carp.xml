<Profile>
  <Name>Ghost Carp</Name>
  <BehaviorDirectory></BehaviorDirectory>
  <Order>
  
	<If condition="not HasAtLeast(7700,1)">
		<Log Message="Start Fishing: Ghost Carp..." />
		<If condition="not HasAtLeast(2594,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2594,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If condition="not IsOnSubMap(70)">
			<LisbethTravel ZoneId="148" XYZ="211.2545, -32.20573, 395.1728"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<ExFish Bait="Butterworm" Condition="not HasAtLeast(7700,1) and IsTimeBetween(21,3) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(148, 'Rain')">
			<Keepers>
				<Keeper name="Ghost Carp" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="230.0548, -31.34614, 387.5831" Heading="1,978786" Sit="True"/>
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
  </Order>
  <GrindAreas>
  </GrindAreas>
  <CodeChunks>
  </CodeChunks>
</Profile>