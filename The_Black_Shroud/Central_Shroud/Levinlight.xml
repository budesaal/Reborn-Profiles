<?xml version="1.0" encoding="utf-8"?>
<!--
	Profile: Levinlight (7920)
	Authors: budesaal
-->
<Profile>
	<Name>Levinlight</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

		<If condition="not HasAtLeast(7920,1)">
			<Log Message="Start Fishing: Levinlight..." />
			<If condition="not HasAtLeast(2597,50)">
				<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2597,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
			</If>
			<If condition="not IsOnSubMap(70)">
				<LisbethTravel ZoneId="148" XYZ="-315.058, 21.97407, 147.0868"/>
			</If>
			<If Condition="not ClassName == ClassJobType.Fisher">
				<ChangeClass Job="Fisher" />
			</If>
			<While Condition="not IsTimeBetween(18,23)" >
				<WaitTimer WaitTime="30" />
			</While>
			<ExFish Bait="Syrphid Basket" Condition="not HasAtLeast(7920,1) and IsTimeBetween(18,23)"
					MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
				<Keepers>
					<Keeper name="Levinlight" />
				</Keepers>
				<FishSpots>
					<FishSpot XYZ="-311.5812, 20.99179, 154.1951" Heading="0.3403623" Sit="True" />
					<FishSpot XYZ="-295.7599, 20.99179, 169.1394" Heading="5.806733" Sit="True" />
					<FishSpot XYZ="-303.1868, 21, 193.6298" Heading="2.94788" Sit="True" />
				</FishSpots>
			</ExFish>
		</If>
		<LLoadProfile Path="../../Start.xml"/>

	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>