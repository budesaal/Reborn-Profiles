			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2597,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If condition="not IsOnSubMap(70)">
			<LisbethTravel ZoneId="148" XYZ="-315.058, 21.97407, 147.0868"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<ExFish Bait="Syrphid Basket" Condition="not HasAtLeast(7920,1) and IsTimeBetween(18,23)"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True">
			<Keepers>
				<Keeper name="Levinlight" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-297.939, 20.99179, 154.5997" Heading="0,9582469" Sit="True"/>
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
  </Order>
