<Profile>
	<Name>Ndendecki</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

		<If condition="not HasAtLeast(8765,1)">
			<Log Message="Start Fishing: Ndendecki..." />
			<If condition="not HasAtLeast(2599,50)">
				<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2599,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
			</If>
			<If condition="not IsOnSubMap(275)">
				<LisbethTravel Area="Western Thanalan" XYZ="-220.5043, 15.77091, -736.3502"/>
			</If>
			<If Condition="not ClassName == ClassJobType.Fisher">
				<ChangeClass Job="Fisher" />
			</If>
			<While Condition="IsTimeBetween(17,18)" >
				<WaitTimer WaitTime="30" />
			</While>
			<ExFish Bait="Honey Worm" Condition="HasAtLeast(2599,1) and not HasAtLeast(8765,1) and (IsTimeBetween(18,24) or IsTimeBetween(0,5)) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(140, 'Fog')"
					MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True"
					Mooch="3" Patience="Patience2" MinimumGPPatience="650" ForceCordial="True" CordialType="Auto">
				<PatienceTugs>
					<PatienceTug moochLevel="0" TugType="Light" />
					<PatienceTug moochLevel="0" TugType="Medium" />
					<PatienceTug moochLevel="1" TugType="Heavy" />
					<PatienceTug moochLevel="2" TugType="Heavy" />
				</PatienceTugs>
				<Keepers>
					<Keeper name="Ndendecki" />
				</Keepers>
				<FishSpots>
					<FishSpot XYZ="-212.2421, 14.67033, -736.3536" Heading="1.601916" Sit="True" />
					<FishSpot XYZ="-180.7458, 14.63993, -742.4382" Heading="0.525413" Sit="True" />
					<FishSpot XYZ="-176.5261, 14.63993, -720.3877" Heading="3.323525" Sit="True" />
				</FishSpots>
			</ExFish>
		</If>
		<LLoadProfile Path="../../Start.xml"/>

	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>