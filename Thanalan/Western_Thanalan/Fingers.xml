<Profile>
	<Name>Fingers</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

		<If condition="not HasAtLeast(7906,1)">
			<Log Message="Start Fishing: Fingers..." />
			<If condition="not HasAtLeast(2606,50)">
				<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2606,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
			</If>
			<If condition="not IsOnSubMap(370)">
				<LisbethTravel Area="Western Thanalan" XYZ="-328.2706, 13.9835, -100.0699"/>
			</If>
			<If Condition="not ClassName == ClassJobType.Fisher">
				<ChangeClass Job="Fisher" />
			</If>
			<While Condition="not IsTimeBetween(17,18)" >
				<WaitTimer WaitTime="30" />
			</While>
			<ExFish Bait="Northern Krill" Condition="not HasAtLeast(7906,1) and IsTimeBetween(17,18)"
