<Profile>
  <Name>Mud Golem</Name>
  <BehaviorDirectory></BehaviorDirectory>
  <Order>
  
	<If condition="not HasAtLeast(7706,1)">
		<Log Message="Start Fishing: Mud Golem..." />
		<If condition="not HasAtLeast(2594,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2594,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If condition="not IsOnSubMap(245)">
			<LisbethTravel Area="Western Thanalan" XYZ="-196.8338, 15.78721, -273.8176"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While Condition="not IsTimeBetween(21,24) or not IsTimeBetween(0,3)" >
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Butterworm" Condition="not HasAtLeast(7706,1) and (not IsTimeBetween(21,24) or not IsTimeBetween(0,3))"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True">
			<Keepers>
				<Keeper name="Mud Golem" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-198.4468, 14.51385, -287.4224" Heading="3,295548" Sit="True"/>
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
  </Order>
  <GrindAreas>
  </GrindAreas>
  <CodeChunks>
  </CodeChunks>
</Profile>