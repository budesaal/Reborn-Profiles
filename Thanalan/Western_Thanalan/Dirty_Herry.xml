<Profile>
	<Name>Dirty Herry</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

		<If condition="not HasAtLeast(7910,1)">
			<Log Message="Start Fishing: Dirty Herry..." />
			<If condition="not HasAtLeast(2606,50)">
				<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2606,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
			</If>
			<If condition="not IsOnSubMap(266)">
				<LisbethTravel Area="Western Thanalan" XYZ="-323.066, 32.16915, 385.4688"/>
			</If>
			<If Condition="not ClassName == ClassJobType.Fisher">
				<ChangeClass Job="Fisher" />
			</If>
			<While Condition="not IsTimeBetween(20,22)" >
				<WaitTimer WaitTime="30" />
			</While>
			<ExFish Bait="Northern Krill" Condition="not HasAtLeast(7910,1) and IsTimeBetween(20,22)"
					MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True"
					Mooch="0" Patience="Patience" MinimumGPPatience="600" ForceCordial="True" CordialType="HiCordial">
				<PatienceTugs>
					<PatienceTug moochLevel="0" TugType="Heavy" />
				</PatienceTugs>
				<Keepers>
					<Keeper name="Dirty Herry" />
				</Keepers>
				<FishSpots>
					<FishSpot XYZ="-334.7412, 32.58686, 390.4811" Heading="4.975707" Sit="True" />
					<FishSpot XYZ="-325.4308, 31.86579, 406.1303" Heading="5.476267" Sit="True" />
					<FishSpot XYZ="-317.0293, 32.65864, 421.4394" Heading="4.198692" Sit="True" />
				</FishSpots>
			</ExFish>
		</If>
		<LLoadProfile Path="../../Start.xml"/>

	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>