			<FishSpots>
				<FishSpot XYZ="250.9285, -3.368649, 50.39566" Heading="1,908023" Sit="True"/>
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
