				<FishSpot XYZ="166.1766, -28.29997, -426.6662" Heading="0.5941291" Sit="True" />
				<FishSpot XYZ="197.2962, -28.24627, -422.0775" Heading="0.1082254" Sit="True" />
			</FishSpots>
