<Profile>
	<Name>Kuno the Killer</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(8763,1)">
		<Log Message="Start Fishing: Kuno the Killer..." />
		<If condition="not HasAtLeast(2599,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2599,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If condition="not IsOnSubMap(410)">
			<LisbethTravel Area="Mor Dhona" XYZ="454.4445, -4.775482, -750.3284"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While Condition="IsTimeBetween(7,8)">
			<WaitTimer WaitTime="30" />
		</While>
		<While Condition="HasAtLeast(2599,1) and not HasAtLeast(8763,1) and IsTimeBetween(8,18) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(156, 'Gloom')">
			<!-- 1x Canavan for Fisher's Intuition -->
			<ExFish Bait="Honey Worm" Condition="not Core.Player.HasAura(568)"
					MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True"
					Mooch="3" Patience="Patience" MinimumGPPatience="600" ForceCordial="True" CordialType="HiCordial">
				<PatienceTugs>
					<PatienceTug moochLevel="0" TugType="Light" />
					<PatienceTug moochLevel="1" TugType="Light" />
					<PatienceTug moochLevel="2" TugType="Heavy" />
				</PatienceTugs>
				<Keepers>
					<Keeper name="Canavan" />
				</Keepers>
				<FishSpots>
					<FishSpot XYZ="445.7883, -5.563284, -750.0673" Heading="5.845725" Sit="True" />
					<FishSpot XYZ="460.5209, -4.923997, -748.8826" Heading="1.007671" Sit="True" />
					<FishSpot XYZ="490.8825, -5.542807, -753.9091" Heading="5.38077" Sit="True" />
				</FishSpots>
			</ExFish>
			<!-- Now go for Kuno the Killer -->
			<ExFish Bait="Honey Worm" Condition="Core.Player.HasAura(568)"
					MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True"
					Mooch="3" Patience="Patience" MinimumGPPatience="600" ForceCordial="True" CordialType="HiCordial">
				<PatienceTugs>
					<PatienceTug moochLevel="0" TugType="Light" />
					<PatienceTug moochLevel="1" TugType="Medium" />
					<PatienceTug moochLevel="2" TugType="Heavy" />
				</PatienceTugs>
				<Keepers>
					<Keeper name="Kuno the Killer" />
				</Keepers>
				<SurfaceSlaps>
					<SurfaceSlap name="Lamp Marimo"/>
				</SurfaceSlaps>
				<FishSpots>
					<FishSpot XYZ="445.7883, -5.563284, -750.0673" Heading="5.845725" Sit="True" />
					<FishSpot XYZ="460.5209, -4.923997, -748.8826" Heading="1.007671" Sit="True" />
					<FishSpot XYZ="490.8825, -5.542807, -753.9091" Heading="5.38077" Sit="True" />
				</FishSpots>
			</ExFish>
		</While>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>