<Profile>
	<Name>Octomammoth</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(7707,1)">
		<Log Message="Start Fishing: Octomammoth..." />
		<If condition="not HasAtLeast(2587,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2587,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If condition="not IsOnSubMap(0)">
			<LisbethTravel Area="Limsa Lominsa (Lower Decks)" XYZ="-183.9088, 2, 194.1954"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While Condition="IsTimeBetween(8,9)">
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Pill Bug" Condition="HasAtLeast(2587,1) and not HasAtLeast(7707,1) and IsTimeBetween(9,17)"
				MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True"
				Mooch="1" Patience="Patience2" MinimumGPPatience="650" ForceCordial="True" CordialType="Auto">
			<PatienceTugs>
				<PatienceTug moochLevel="0" TugType="Medium" />
			</PatienceTugs>
			<Keepers>
				<Keeper name="Octomammoth" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-182.248, 1.999995, 200.2238" Heading="1.683593" Sit="True" />
				<FishSpot XYZ="-192.3636, 0.9999907, 219.1647" Heading="5.947781" Sit="True" />
				<FishSpot XYZ="-170.0204, 1.999995, 221.3403" Heading="2.986306" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>