<Profile>
	<Name>Thundergut</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(7937,1)">
		<Log Message="Start Fishing: Thundergut..." />
		<If condition="not HasAtLeast(2601,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2601,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If condition="not IsOnSubMap(235)">
			<LisbethTravel Area="Outer La Noscea" XYZ="-330.9283, 32.90812, -470.4216" />
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While Condition="IsTimeBetween(3,19)">
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Stem Borer" Condition="not HasAtLeast(7937,1) and (not IsTimeBetween(19,24) or not IsTimeBetween(0,3)) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(180, 'Rain')"
				MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
			<Keepers>
				<Keeper name="Thundergut" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-340.5363, 38.69806, -449.6728" Heading="0.5665219" Sit="True" />
				<FishSpot XYZ="-329.0862, 34.76004, -457.2785" Heading="2.843106" Sit="True" />
				<FishSpot XYZ="-323.2883, 32.5542, -457.3354" Heading="2.960396" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
