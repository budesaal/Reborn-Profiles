<Profile>
	<Name>The Greatest Bream in the World</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(7909,1)">
		<Log Message="Start Fishing: The Greatest Bream in the World..." />
		<If condition="not HasAtLeast(2613,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2613,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<If condition="not IsOnSubMap(216)">
			<LisbethTravel Area="Eastern La Noscea" XYZ="535.896, 8.662354, 619.4787"/>
		</If>
		<While Condition="IsTimeBetween(19,20)">
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Shrimp Cage Feeder" Condition="HasAtLeast(2613,1) and not HasAtLeast(7909,1) and IsTimeBetween(20,23) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(137, 'Clear Skies', 'Fair Skies')"
				MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
			<Keepers>
				<Keeper name="The Greatest Bream in the World" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="535.7999, 8.662571, 619.3949" Heading="0.8534974" Sit="True" />
				<FishSpot XYZ="524.1935, 8.669197, 646.7106" Heading="0.958216" Sit="True" />
				<FishSpot XYZ="518.6304, 8.678953, 675.7701" Heading="6.229812" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>