<Profile>
	<Name>The Salter</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(7925,1)">
		<Log Message="Start Fishing: The Salter..." />
		<If condition="not HasAtLeast(2599,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2599,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<If condition="not IsOnSubMap(216)">
			<LisbethTravel Area="Eastern La Noscea" XYZ="466.6412, 8.396084, 76.86406"/>
		</If>
		<While Condition="not IsTimeBetween(17,20)">
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Honey Worm" Condition="not HasAtLeast(7925,1) and IsTimeBetween(17,20) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(137, 'Clear Skies', 'Fair Skies')"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True">
			<Keepers>
				<Keeper name="The Salter" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="468.0401, 8.434704, 76.06626" Heading="2.070008" Sit="True" />
				<FishSpot XYZ="494.2133, 8.734549, 67.54633" Heading="0.07195473" Sit="True" />
				<FishSpot XYZ="469.8491, 8.482638, 88.38069" Heading="3.607294" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>