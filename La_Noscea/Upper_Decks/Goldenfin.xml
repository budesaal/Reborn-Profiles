<Profile>
	<Name>Goldenfin</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(7685,1)">
		<Log Message="Start Fishing: Goldenfin..." />
		<If condition="not HasAtLeast(2587,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2587,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If condition="not IsOnSubMap(715)">
			<LisbethTravel Area="Limsa Lominsa (Upper Decks)" XYZ="19.86234, 40, 76.17139"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While Condition="not IsTimeBetween(9,14)">
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Pill Bug" Condition="not HasAtLeast(7685,1) and IsTimeBetween(9,14)"
				MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
			<Keepers>
				<Keeper name="Goldenfin" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="39.98388, 39.99995, 75.70851" Heading="0.08564156" Sit="True" />
				<FishSpot XYZ="58.56662, 39.99997, 70.25841" Heading="2.929538" Sit="True" />
				<FishSpot XYZ="53.64757, 39.99995, 75.66903" Heading="5.981073" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>