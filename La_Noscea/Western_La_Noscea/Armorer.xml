<Profile>
	<Name>Armorer</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(7687,1)">
		<Log Message="Start Fishing: Armorer..." />
		<If condition="not HasAtLeast(2606,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2606,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<If condition="not IsOnSubMap(218)">
			<LisbethTravel Zone="138" XYZ="658.0223, 9.094515, 510.2093" />
		</If>
		<While Condition="IsTimeBetween(18,19)">
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Northern Krill" Condition="HasAtLeast(2606,1) and not HasAtLeast(7687,1) and (IsTimeBetween(19,24) or IsTimeBetween(0,3))"
				MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
			<Keepers>
				<Keeper name="Armorer" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="645.257, 5.578633, 556.2516" Heading="5.217016" Sit="True" />
				<FishSpot XYZ="604.5823, 5.038345, 492.6462" Heading="0.09802127" Sit="True" />
				<FishSpot XYZ="591.8521, 4.822018, 471.4789" Heading="5.331914" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>