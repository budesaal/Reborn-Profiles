<Profile>
	<Name>Endoceras</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(8756,1)">
		<Log Message="Start Fishing: Endoceras..." />
		<If condition="not HasAtLeast(2596,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2596,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<If condition="not IsOnSubMap(228)">
			<LisbethTravel Area="Western La Noscea" XYZ="-464.1351, -38.0391, 737.9131"/>
		</If>
		<While Condition="not IsTimeBetween(20,4)">
			<WaitTimer WaitTime="3" />
		</While>
		<ExFish Bait="Spoon Worm" Condition="not HasAtLeast(8756,1) and IsTimeBetween(20,4) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(138, 'Clouds', 'Fog', 'Wind') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(138, TimeSpan.FromHours(-8), 'Clear Skies', 'Fair Skies')"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True"
				Mooch="2" Patience="Patience" MinimumGPPatience="500" ForceCordial="True" CordialType="HiCordial">
			<PatienceTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Heavy" />
			</PatienceTugs>
			<Keepers>
				<Keeper name="Endoceras" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-453.0573, -37.94336, 745.8951" Heading="0.1122043" Sit="True" />
				<FishSpot XYZ="-475.5448, -38.35024, 768.3134" Heading="6.152446" Sit="True" />
				<FishSpot XYZ="-490.0315, -37.92073, 761.21" Heading="5.404744" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>