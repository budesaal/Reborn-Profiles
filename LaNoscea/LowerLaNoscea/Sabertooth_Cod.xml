<Profile>
  <Name>Sabertooth Cod</Name>
  <BehaviorDirectory></BehaviorDirectory>
  <Order>
  
	<If condition="not HasAtLeast(7691,1)">
		<Log Message="Start Fishing: Sabertooth Cod..." />
		<If condition="not HasAtLeast(2589,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2589,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<If condition="not IsOnSubMap(339)">
			<LisbethTravel ZoneId="135" XYZ="-97.83857, 4.310742, 853.5196"/>
		</If>
		<ExFish Bait="Goby Ball" Condition="not HasAtLeast(7691,1) and IsTimeBetween(16,22)"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69">
			<Keepers>
				<Keeper name="Sabertooth Cod" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-132.2859, 4.310742, 860.4258" Heading="0.04101062" Sit="True" />
				<FishSpot XYZ="-79.52073, 5.327857, 860.4695" Heading="6.221565" Sit="True" />
				<FishSpot XYZ="-93.51559, 4.310742, 846.0161" Heading="3.272657" Sit="True" />
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
  </Order>
  <GrindAreas>
  </GrindAreas>
  <CodeChunks>
  </CodeChunks>
</Profile>