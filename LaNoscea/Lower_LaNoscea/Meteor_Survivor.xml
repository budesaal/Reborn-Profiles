<Profile>
	<Name>Meteor Survivor</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(7903,1)">
		<Log Message="Start Fishing: Meteor Survivor..." />
		<If condition="not HasAtLeast(2591,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2591,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<If condition="not IsOnSubMap(338)">
			<LisbethTravel ZoneId="135" XYZ="-185.0453, -0.3133438, 727.9094"/>
		</If>
		<While Condition="not IsTimeBetween(3,5)">
			<WaitTimer WaitTime="3" />
		</While>
		<ExFish Bait="Rat Tail" Condition="not HasAtLeast(7903,1) and IsTimeBetween(3,5) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(135, 'Clouds', 'Fog', 'Wind')"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
			<Keepers>
				<Keeper name="Meteor Survivor" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-166.2906, -0.271, 748.7253" Heading="1.937045" Sit="True" />
				<FishSpot XYZ="-196.9584, -0.1789726, 700.2988" Heading="4.19792" Sit="True" />
				<FishSpot XYZ="-232.1735, -0.2515026, 666.2292" Heading="5.016819" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>