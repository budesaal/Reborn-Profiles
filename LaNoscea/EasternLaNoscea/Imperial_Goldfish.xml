<Profile>
  <Name>Imperial Goldfish</Name>
  <BehaviorDirectory></BehaviorDirectory>
  <Order>
  
	<If condition="not HasAtLeast(8752,5)">
		<Log Message="Start Fishing: Imperial Goldfish..." />
		<If condition="not HasAtLeast(2599,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2599,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<If condition="not IsOnSubMap(168)">
			<LisbethTravel ZoneId="137" XYZ="-253.4813, 35.16173, 289.4978"/>
		</If>
		<ExFish Bait="Honey Worm" Condition="not HasAtLeast(8752,1)"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69"
				Mooch="2" Patience="Patience" MinimumGPPatience="300" ForceCordial="True">
			<PatienceTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Medium" />
			</PatienceTugs>
			<Keepers>
				<Keeper name="Imperial Goldfish" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-286.5046, 34.35244, 291.675" Heading="4.773697" Sit="True" />
				<FishSpot XYZ="-287.1476, 34.56785, 311.0092" Heading="4.249032" Sit="True" />
				<FishSpot XYZ="-304.5351, 35.64708, 273.6548" Heading="5.947596" Sit="True" />
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
  </Order>
  <GrindAreas>
  </GrindAreas>
  <CodeChunks>
  </CodeChunks>
</Profile>