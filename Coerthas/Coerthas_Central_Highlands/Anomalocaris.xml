			<PatienceTugs>
				<PatienceTug moochLevel="0" TugType="Medium" />
				<PatienceTug moochLevel="1" TugType="Heavy" />
			</PatienceTugs>
			<Keepers>
				<Keeper name="Anomalocaris" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-448.98, 200.6491, -369.0937" Heading="2.616056" Sit="True" />
				<FishSpot XYZ="-433.0484, 200.7377, -358.037" Heading="1.987735" Sit="True" />
				<FishSpot XYZ="-476.9392, 200.8663, -388.2444" Heading="2.457772" Sit="True" />
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>