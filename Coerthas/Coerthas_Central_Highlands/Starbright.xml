<Profile>
	<Name>Starbright</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(7945,1)">
		<Log Message="Start Fishing: Starbright..." />
		<If condition="not HasAtLeast(2597,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':2597,'Amount':50,'Enabled':true,'Type':'Purchase'}]"/>
		</If>
		<If condition="not IsOnSubMap(383)">
			<LisbethTravel Area="Coerthas Central Highlands" XYZ="-299.6851, 244.8674, 12.87033"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While Condition="not IsTimeBetween(21,24) or not IsTimeBetween(0,4)">
			<WaitTimer WaitTime="30" />
		</While>
		<ExFish Bait="Syrphid Basket" Condition="not HasAtLeast(7945,1) and (not IsTimeBetween(21,24) or not IsTimeBetween(0,4)) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(155, 'Clear Skies', 'Fair Skies')"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True"
				Mooch="1" Patience="Patience" MinimumGPPatience="500" ForceCordial="True" CordialType="HiCordial">
			<PatienceTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Heavy" />
			</PatienceTugs>
			<Keepers>
				<Keeper name="Starbright" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-292.2026, 242.4896, 17.62959" Heading="1.029324" Sit="True" />
				<FishSpot XYZ="-269.9615, 248.1604, 10.56468" Heading="0.02401352" Sit="True" />
				<FishSpot XYZ="-299.8128, 242.6337, 32.33716" Heading="1.896401" Sit="True" />
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>