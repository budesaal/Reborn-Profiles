<Profile>
	<Name>Bishopfish</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(17577,1)">
		<Log Message="Start Fishing: Bishopfish..." />
		<If condition="not HasAtLeast(12711,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':12711,'Amount':50,'Enabled':true,'Type':'Exchange'}]"/>
		</If>
		<If condition="not IsOnSubMap(2201)">
			<LisbethTravel ZoneId="397" XYZ="692.1117, 132.263, -797.5139"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While Condition="not IsTimeBetween(10,14)">
			<WaitTimer WaitTime="3" />
		</While>
		<ExFish Bait="Brute Leech" Condition="not HasAtLeast(17577,1) and IsTimeBetween(10,14) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(397, 'Clear Skies')"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69">
			<Keepers>
				<Keeper name="Bishopfish" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="702.2099, 130.5252, -810.4643" Heading="8,382218" Sit="True"/>
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>