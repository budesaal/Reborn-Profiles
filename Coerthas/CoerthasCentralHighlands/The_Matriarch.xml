	<LLoadProfile Path="../../Start.xml"/>

	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
		<CodeChunk Name="Thaliaks_Favor">
			<![CDATA[
				if ((Core.Me.MaxGP - Core.Me.CurrentGP) > 200 && ActionManager.CanCast(26804, Core.Me))
					{
    					ActionManager.DoAction(26804, Core.Me);
					}
			]]>
		</CodeChunk>
	</CodeChunks>
</Profile>