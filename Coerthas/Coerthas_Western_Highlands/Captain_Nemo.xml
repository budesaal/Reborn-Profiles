<Profile>
	<Name>Captain Nemo</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>

	<If condition="not HasAtLeast(17578,1)">
		<Log Message="Start Fishing: Captain Nemo..." />
		<If condition="not HasAtLeast(12711,50)">
			<Lisbeth IgnoreHome="True" Json="[{'Group':1,'Item':12711,'Amount':50,'Enabled':true,'Type':'Exchange'}]"/>
		</If>
		<If condition="not IsOnSubMap(2202)">
			<LisbethTravel Area="Coerthas Western Highlands" XYZ="-12.63069, 90.64453, -557.0477"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<ExFish Bait="Brute Leech" Condition="not HasAtLeast(17578,1) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(397, 'Blizzards') and ExBuddy.Helpers.SkywatcherPlugin.PredictWeatherInZone(397, TimeSpan.FromHours(-8), 'Snow')"
				MinFish="15" MaxFish="20" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
			<Keepers>
				<Keeper name="Captain Nemo" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-23.51305, 88.99787, -555.3638" Heading="4.676697" Sit="True" />
				<FishSpot XYZ="-33.74816, 89.23457, -571.0184" Heading="0.203069" Sit="True" />
				<FishSpot XYZ="-49.15779, 89.21899, -555.2145" Heading="1.220945" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
</Profile>