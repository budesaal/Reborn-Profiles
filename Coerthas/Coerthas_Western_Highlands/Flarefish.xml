			<PatienceTugs>
				<PatienceTug moochLevel="0" TugType="Light" />
				<PatienceTug moochLevel="1" TugType="Heavy" />
			</PatienceTugs>
			<Keepers>
				<Keeper name="Flarefish" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="276.9553, 131.7509, -237.6015" Heading="5.675732" Sit="True" />
				<FishSpot XYZ="252.1819, 131.7576, -235.2873" Heading="1.013605" Sit="True" />
				<FishSpot XYZ="240.2967, 132.1151, -206.4833" Heading="1.130893" Sit="True" />
			</FishSpots>
		</ExFish>
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
	</Order>
	<GrindAreas>
	</GrindAreas>
	<CodeChunks>
	</CodeChunks>
