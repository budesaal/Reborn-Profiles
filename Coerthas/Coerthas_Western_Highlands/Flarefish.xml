		<If condition="not IsOnSubMap(2201)">
			<LisbethTravel Area="Coerthas Western Highlands" XYZ="280.3072, 132.3909, -244.9158"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<While Condition="not IsTimeBetween(10,16)">
			<WaitTimer WaitTime="3" />
		</While>
		<ExFish Bait="Purse Web Spider" Condition="not HasAtLeast(16746,1) and IsTimeBetween(10,16) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(397, 'Blizzards')"
