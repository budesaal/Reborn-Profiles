<Profile>
  <Name>Floating Boulder</Name>
  <BehaviorDirectory></BehaviorDirectory>
  <Order>
  
	<If condition="not HasAtLeast(7913,1)">
		<Log Message="Start Fishing: Floating Boulder..." />
		<If condition="not HasAtLeast(2595,50)">
			<Lisbeth Json="[{'Item': 2595,'Group': 0,'Amount':50,'Collectable': false,'QuickSynth': false,'SuborderQuickSynth': false,'Hq': false,'Food': 0,'Primary': true,'Type': 'Culinarian','Enabled': true,'Manual': 0,'Medicine': 0}]"/>
		</If>
		<If condition="not IsOnSubMap(79)">
			<LisbethTravel ZoneId="153" XYZ="-164.9751, 2.177883, 171.357"/>
		</If>
		<If Condition="not ClassName == ClassJobType.Fisher">
			<ChangeClass Job="Fisher" />
		</If>
		<ExFish Bait="Bass Ball" Condition="not HasAtLeast(7913,1) and IsTimeBetween(0,8) and ExBuddy.Helpers.SkywatcherPlugin.IsWeatherInZone(153, 'Clouds', 'Fog')"
				MinFish="10" MaxFish="15" ShuffleFishSpots="True" SitRate=".69" ThaliaksFavor="True" Chum="True">
			<Keepers>
				<Keeper name="Floating Boulder" />
			</Keepers>
			<FishSpots>
				<FishSpot XYZ="-162.6687, -0.2929233, 156.6938" Heading="2,689708" Sit="True"/>
			</FishSpots>
		</ExFish>	
	</If>
	<LLoadProfile Path="../../Start.xml"/>
	
  </Order>
  <GrindAreas>
  </GrindAreas>
  <CodeChunks>
  </CodeChunks>
</Profile>