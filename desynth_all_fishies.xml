<?xml version="1.0" encoding="utf-8"?>
<!--
	Profile: Desynth All Fishies (Big and Small)
	Authors: miss-aoi
	Wil desynth all Big Fish - you have to do smalls yourself.
	Be sure to update "1" as needed in Start.xml as needed
-->
<Profile>
	<Name>Desynth All Fishies</Name>
	<BehaviorDirectory></BehaviorDirectory>
	<Order>
		<If Condition="HasAtLeast(4925,1)"> <!-- Crayfish -->
			<Desynth ItemIds="4925" DesynthDelay="6000" DesynthTimefile="10"/>
		</If>
		<If Condition="HasAtLeast(4927,1)"> <!-- Striped Goby -->
			<Desynth ItemIds="4927" DesynthDelay="6000" DesynthTimefile="10"/>
		</If>
		<If Condition="HasAtLeast(4932,1)"> <!-- Pipira -->
			<Desynth ItemIds="4932" DesynthDelay="6000" DesynthTimefile="10"/>
